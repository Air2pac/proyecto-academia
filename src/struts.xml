		<action name="AbrirModificarProfesor" class="dao.ProfesorDAO" method="abrirModificarProfesor">
			<result name="success">jsp/modificarProfesor.jsp</result>
		</action>
		<action name="ModificarProfesor" class="dao.ProfesorDAO" method="modificarProfesor">
			<result name="success">jsp/mantenimientoProfesores.jsp</result>
			<result name="input">jsp/modificarProfesor.jsp</result>
		</action>
		<action name="AbrirAddProAsign" class="dao.ProfesorDAO" method="abrirAddProAsign">
			<result name="success">jsp/addProAsignatura.jsp</result>
		</action>
		<action name="AddProAsign" class="dao.ProfesorDAO" method="addProAsign">
			<result name="success">jsp/mantenimientoProfesores.jsp</result>
			<result name="input">jsp/addProAsignatura.jsp</result>
